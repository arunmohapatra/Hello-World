import java.io.*;
import java.util.*;

public class day8 {

    static class Pair implements Comparable<Pair> {
        double dist;
        int a, b;

        Pair(double d, int x, int y) {
            dist = d;
            a = x;
            b = y;
        }

        @Override
        public int compareTo(Pair other) {
            return Double.compare(this.dist, other.dist);
        }
    }

    static class DSU {
        int[] parent;
        int[] size;

        DSU(int n) {
            parent = new int[n];
            size = new int[n];
            for (int i = 0; i < n; i++) {
                parent[i] = i;
                size[i] = 1;
            }
        }

        int find(int x) {
            if (parent[x] != x)
                parent[x] = find(parent[x]);
            return parent[x];
        }

        void union(int a, int b) {
            int pa = find(a);
            int pb = find(b);
            if (pa != pb) {
                parent[pb] = pa;
                size[pa] += size[pb];
            }
        }
    }

    public static void main(String[] args) throws Exception {

        // Read file named "input"
        File file = new File("D:\\input.txt");
        BufferedReader br = new BufferedReader(new FileReader(file));

        List<int[]> points = new ArrayList<>();
        String line;

        while ((line = br.readLine()) != null) {
            line = line.trim();
            if (line.isEmpty()) continue;
            String[] parts = line.split(",");
            points.add(new int[]{
                    Integer.parseInt(parts[0].trim()),
                    Integer.parseInt(parts[1].trim()),
                    Integer.parseInt(parts[2].trim())
            });
        }
        br.close();

        int n = points.size();
        List<Pair> pairs = new ArrayList<>();

        // Compute all distances
        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                int[] A = points.get(i);
                int[] B = points.get(j);

                double dx = A[0] - B[0];
                double dy = A[1] - B[1];
                double dz = A[2] - B[2];
                double dist = Math.sqrt(dx * dx + dy * dy + dz * dz);

                pairs.add(new Pair(dist, i, j));
            }
        }

        // Sort pairs by distance
        Collections.sort(pairs);

        DSU dsu = new DSU(n);

        // Process exactly 1000 closest pairs
        int LIMIT = Math.min(1000, pairs.size());

        for (int i = 0; i < LIMIT; i++) {
            Pair p = pairs.get(i);
            // Merge only if not already connected
            if (dsu.find(p.a) != dsu.find(p.b)) {
                dsu.union(p.a, p.b);
            }
        }

        // Count components
        HashMap<Integer, Integer> comp = new HashMap<>();
        for (int i = 0; i < n; i++) {
            int root = dsu.find(i);
            comp.put(root, comp.getOrDefault(root, 0) + 1);
        }

        // Get the 3 largest
        List<Integer> sizes = new ArrayList<>(comp.values());
        sizes.sort(Collections.reverseOrder());

        int answer = 1;
        for (int i = 0; i < Math.min(3, sizes.size()); i++) {
            answer *= sizes.get(i);
        }

        System.out.println(answer);
    }
}
