import java.io.*;
import java.util.*;

public class day8 {

    static class Edge implements Comparable<Edge> {
        final int a, b;
        final long dist2;
        Edge(int a, int b, long dist2) { this.a = a; this.b = b; this.dist2 = dist2; }
        @Override
        public int compareTo(Edge o) {
            if (this.dist2 != o.dist2) return Long.compare(this.dist2, o.dist2);
            if (this.a != o.a) return Integer.compare(this.a, o.a);
            return Integer.compare(this.b, o.b);
        }
    }

    static class DSU {
        int[] parent;
        int[] size;
        DSU(int n) {
            parent = new int[n];
            size = new int[n];
            for (int i = 0; i < n; i++) { parent[i] = i; size[i] = 1; }
        }
        int find(int x) {
            if (parent[x] != x) parent[x] = find(parent[x]);
            return parent[x];
        }
        // returns true if merged (were in different sets)
        boolean union(int a, int b) {
            int ra = find(a), rb = find(b);
            if (ra == rb) return false;
            if (size[ra] < size[rb]) { int t = ra; ra = rb; rb = t; }
            parent[rb] = ra;
            size[ra] += size[rb];
            return true;
        }
    }

    public static void main(String[] args) throws Exception {
        // Read file named "input"
        File f = new File("D:\\input.txt");
        if (!f.exists()) {
            System.err.println("input file not found");
            return;
        }

        List<int[]> pts = new ArrayList<>();
        try (BufferedReader br = new BufferedReader(new FileReader(f))) {
            String line;
            while ((line = br.readLine()) != null) {
                line = line.trim();
                if (line.isEmpty()) continue;
                String[] parts = line.split(",");
                if (parts.length != 3) continue;
                try {
                    int x = Integer.parseInt(parts[0].trim());
                    int y = Integer.parseInt(parts[1].trim());
                    int z = Integer.parseInt(parts[2].trim());
                    pts.add(new int[]{x, y, z});
                } catch (NumberFormatException ignored) {
                    // skip invalid line
                }
            }
        }

        int n = pts.size();
        if (n < 2) {
            System.out.println(0);
            return;
        }

        List<Edge> edges = new ArrayList<>();
        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                long dx = (long)pts.get(i)[0] - pts.get(j)[0];
                long dy = (long)pts.get(i)[1] - pts.get(j)[1];
                long dz = (long)pts.get(i)[2] - pts.get(j)[2];
                long d2 = dx*dx + dy*dy + dz*dz;
                edges.add(new Edge(i, j, d2));
            }
        }

        Collections.sort(edges);

        DSU dsu = new DSU(n);
        int components = n;
        long finalProduct = 0L;
        for (Edge e : edges) {
            if (dsu.union(e.a, e.b)) {
                components--;
                if (components == 1) {
                    long x1 = pts.get(e.a)[0];
                    long x2 = pts.get(e.b)[0];
                    finalProduct = x1 * x2;
                    break;
                }
            }
        }

        System.out.println(finalProduct);
    }
}
