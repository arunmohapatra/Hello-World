import java.io.*;
import java.util.*;

public class day8 {

    static class Pair implements Comparable<Pair> {
        double dist;
        int a, b;

        Pair(double d, int x, int y) {
            dist = d;
            a = x;
            b = y;
        }

        @Override
        public int compareTo(Pair other) {
            return Double.compare(this.dist, other.dist);
        }
    }

    static class DSU {
        int[] parent;
        int[] size;

        DSU(int n) {
            parent = new int[n];
            size = new int[n];
            for (int i = 0; i < n; i++) {
                parent[i] = i;
                size[i] = 1;
            }
        }

        int find(int x) {
            if (parent[x] != x)
                parent[x] = find(parent[x]);
            return parent[x];
        }

        boolean union(int a, int b) {
            int pa = find(a);
            int pb = find(b);
            if (pa == pb) return false;

            parent[pb] = pa;
            size[pa] += size[pb];
            return true;
        }
    }

    public static void main(String[] args) throws Exception {

        // Read file named "input"
        BufferedReader br = new BufferedReader(new FileReader("I:\\input.txt"));
        List<int[]> points = new ArrayList<>();
        String line;

        while ((line = br.readLine()) != null) {
            line = line.trim();
            if (line.isEmpty()) continue;
            String[] parts = line.split(",");
            points.add(new int[]{
                    Integer.parseInt(parts[0].trim()),
                    Integer.parseInt(parts[1].trim()),
                    Integer.parseInt(parts[2].trim())
            });
        }
        br.close();

        int n = points.size();
        List<Pair> pairs = new ArrayList<>();

        // Compute all pairwise distances
        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                int[] A = points.get(i);
                int[] B = points.get(j);

                double dx = A[0] - B[0];
                double dy = A[1] - B[1];
                double dz = A[2] - B[2];

                double dist = Math.sqrt(dx * dx + dy * dy + dz * dz);

                pairs.add(new Pair(dist, i, j));
            }
        }

        // Sort by distance
        Collections.sort(pairs);

        DSU dsu = new DSU(n);

        int components = n;
        int answer = -1;

        // Connect until one circuit remains
        for (Pair p : pairs) {
            if (dsu.union(p.a, p.b)) {
                components--;

                // The merge that reduces components to 1 is the final required connection.
                if (components == 1) {
                    int x1 = points.get(p.a)[0];
                    int x2 = points.get(p.b)[0];
                    answer = x1 * x2;
                    break;
                }
            }
        }

        System.out.println(answer);
    }
}
