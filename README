import java.io.*;
import java.util.*;

public class FreshIngredientCounter {

    public static void main(String[] args) {

        String filePath = "I:\\input.txt";   // <-- your file location

        List<int[]> freshRanges = new ArrayList<>();
        List<Integer> ingredientIds = new ArrayList<>();

        try (BufferedReader br = new BufferedReader(new FileReader(filePath))) {

            String line;
            boolean readingRanges = true;

            while ((line = br.readLine()) != null) {
                line = line.trim();

                // Blank line separates ranges and ingredient IDs
                if (line.isEmpty()) {
                    readingRanges = false;
                    continue;
                }

                if (readingRanges) {
                    // Parse ranges like "3-5"
                    String[] parts = line.split("-");
                    int start = Integer.parseInt(parts[0]);
                    int end = Integer.parseInt(parts[1]);
                    freshRanges.add(new int[]{start, end});
                } else {
                    // Parse ingredient ID
                    ingredientIds.add(Integer.parseInt(line));
                }
            }

        } catch (IOException e) {
            System.out.println("Error reading file: " + filePath);
            e.printStackTrace();
            return;
        }

        int freshCount = 0;

        for (int id : ingredientIds) {
            if (isFresh(id, freshRanges)) {
                freshCount++;
            }
        }

        System.out.println("Total fresh ingredient IDs: " + freshCount);
    }

    private static boolean isFresh(int id, List<int[]> ranges) {
        for (int[] range : ranges) {
            if (id >= range[0] && id <= range[1]) {
                return true;
            }
        }
        return false;
    }
}
