import java.io.*;
import java.util.*;

public class FreshIngredientCounter {

    public static void main(String[] args) {

        String filePath = "I:\\input.txt";   // <-- your file location

        List<long[]> freshRanges = new ArrayList<>();
        List<Long> ingredientIds = new ArrayList<>();

        try (BufferedReader br = new BufferedReader(new FileReader(filePath))) {

            String line;
            boolean readingRanges = true;

            while ((line = br.readLine()) != null) {
                line = line.trim();

                // Blank line separates ranges and ingredient IDs
                if (line.isEmpty()) {
                    readingRanges = false;
                    continue;
                }

                if (readingRanges) {
                    // Parse ranges like "3-5"
                    String[] parts = line.split("-");

                    try {
                        long start = Long.parseLong(parts[0]);
                        long end = Long.parseLong(parts[1]);
                        freshRanges.add(new long[]{start, end});
                    } catch (NumberFormatException ex) {
                        System.out.println("Invalid range found and skipped: " + line);
                    }

                } else {
                    try {
                        long id = Long.parseLong(line);
                        ingredientIds.add(id);
                    } catch (NumberFormatException ex) {
                        System.out.println("Invalid ingredient ID skipped: " + line);
                    }
                }
            }

        } catch (IOException e) {
            System.out.println("Error reading file: " + filePath);
            e.printStackTrace();
            return;
        }

        int freshCount = 0;

        for (long id : ingredientIds) {
            if (isFresh(id, freshRanges)) {
                freshCount++;
            }
        }

        System.out.println("Total fresh ingredient IDs: " + freshCount);
    }

    private static boolean isFresh(long id, List<long[]> ranges) {
        for (long[] range : ranges) {
            if (id >= range[0] && id <= range[1]) {
                return true;
            }
        }
        return false;
    }
}
