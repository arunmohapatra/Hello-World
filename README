import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;

public class MaxJoltageCalculator {

    public static void main(String[] args) {
        String filePath = "input.txt"; // change to your actual file

        long total = 0;

        try (BufferedReader br = new BufferedReader(new FileReader(filePath))) {
            String line;
            while ((line = br.readLine()) != null) {
                int value = maxTwoDigitInOrder(line);
                total += value;
                System.out.println(line + " -> " + value);
            }
            System.out.println("Total Output Joltage = " + total);
        } catch (IOException e) {
            e.printStackTrace();
        }
    }

    // Find the largest 2-digit number formed by digits in order (i < j)
    private static int maxTwoDigitInOrder(String s) {
        int max = 0;
        int n = s.length();

        for (int i = 0; i < n - 1; i++) {
            int first = s.charAt(i) - '0';

            for (int j = i + 1; j < n; j++) {
                int second = s.charAt(j) - '0';
                int value = first * 10 + second;

                if (value > max) {
                    max = value;
                }
            }
        }
        return max;
    }
}
